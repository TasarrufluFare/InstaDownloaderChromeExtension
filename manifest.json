{
  "name": "InstaDown By TF",
  "description": "Download photos from instagram!",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://www.instagram.com/p/*"
  ],
  "version": "1.0",
  "manifest_version": 2,
  "icons": { "128": "images/icon.png" },
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
